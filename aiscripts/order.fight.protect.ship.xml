		<do_if value="@$kAAITParam_isAvoidHighRisk and (not @global.$kAAITByShipId.{'$' + @this.assignedcontrolled.idcode}.$attackData.$isIgnoreKAAIT)">
			<set_value name="$kAAIT_orderLocation_space" exact="$target.zone" />
			<set_value name="$kAAIT_orderLocation_pos" exact="$pos" />
		</do_if>
	</add>
<!-- add after. purpose: finish.
