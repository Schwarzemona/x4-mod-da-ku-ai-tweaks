<?xml version='1.0' encoding='utf-8'?>
<diff>
  <remove sel="//interrupts" />
  <remove sel="//init" />
  <remove sel="//patch" />
  <replace sel="//attention[@min=&quot;visible&quot;]/actions">
    <actions>
      <label name="start" />
      <run_script name="'order.fight.escort'">
        <param name="target" value="$target" />
      </run_script>
      <label name="finish" />
    </actions>
  </replace>
  <replace sel="//attention[@min=&quot;unknown&quot;]/actions">
    <actions>
      <label name="start" />
      <run_script name="'order.fight.escort'">
        <param name="target" value="$target" />
      </run_script>
      <label name="finish" />
    </actions>
  </replace>
</diff>
