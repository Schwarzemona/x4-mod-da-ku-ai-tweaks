  <add sel="//attention[@min=&quot;visible&quot;]//create_order[@id=&quot;'Attack'&quot;][@object=&quot;$locattacker&quot;][@immediate=&quot;true&quot;]" type="@chance">if @$kAAIT_isSkipAttack then 0 else 100</add>
