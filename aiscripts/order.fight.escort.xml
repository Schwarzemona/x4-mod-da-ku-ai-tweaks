      <create_formation leader="$target" follower="this.assignedcontrolled" formation="$currentformation" param="$formationparam" maxshipsperline="$maxshipsperline" rollmembers="$rollmembers" rollformation="$rollformation"/>
-->
  <add pos="prepend" sel="//attention[@min=&quot;visible&quot;]//create_formation[@leader=&quot;$target&quot;][@follower=&quot;this.assignedcontrolled&quot;]">
    <do_if value="@$kAAITParam_isAvoidHighRisk and (not @this.$kAAIT_data.$isIgnoreKAAIT)">
      <set_value name="$kAAIT_test_space_isAvoid" exact="$target" />
