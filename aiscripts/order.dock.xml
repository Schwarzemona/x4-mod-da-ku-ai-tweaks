<?xml version='1.0' encoding='utf-8'?>
<diff>
	<!-- debug -->
	<add sel="//init">
		<set_value name="$debugchance" exact="100" chance="if $debugchance then 100 else (if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0)" />
	</add>
	<add pos="before" sel="//move_docking">
		<debug_text text="@this.assignedcontrolled.idcode + ' move_docking'" chance="if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0" />
	</add>
	<add pos="before" sel="//run_script[@name=&quot;'move.generic'&quot;][@result=&quot;$movesuccess&quot;]">
		<debug_text text="@this.assignedcontrolled.idcode + ' run_script move.generic'" chance="if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0" />
	</add>
	<!--
		purpose: allow kuertee avoid behaviour
		<label name="start"/>
	-->
	<add pos="before" sel="//label[@name=&quot;start&quot;]">
		<resume label="start" />

		<label name="kAAIT_label_avoid" />
		<debug_text text="@this.assignedcontrolled.idcode + ' LABEL kAAIT_label_avoid'" chance="if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0" />
		<include_interrupt_actions ref="kAAIT_Order_Avoid" />
		<wait min="1s" max="3s" comment="stop this order from continuing as the new order is created" />
		<resume label="start" comment="in case the new order fails, do this" />
	</add>
	<!--
	  add. purpose: cancel waitforsignal on destination when an order changes.
	  (1) <interrupts>
	  (2) <create_order id="'WaitForSignal'" object="$destination" immediate="true">
	  (3) <on_abort>
	-->
	<add sel="//interrupts">
		<library>
			<actions name="CancelWaitOrdersWhenOrderChanges">
				<debug_text text="@this.assignedcontrolled.idcode + ' CancelWaitOrdersWhenOrderChanges $kAAIT_order_waitforsignal: ' + @$kAAIT_order_waitforsignal" chance="if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0" />
				<debug_text text="@this.assignedcontrolled.idcode + ' CancelWaitOrdersWhenOrderChanges $destination.idcode: ' + $destination.idcode + ' $kAAIT_order_waitforsignal: ' + $kAAIT_order_waitforsignal + ' cancel wait for signal'" chance="if @$destination.idcode == @global.$kAAIT.$debugChance_idCode then @global.$kAAIT_Data.$debugChance else 0" />
				<do_if value="@$kAAIT_order_waitforsignal.exists">
					<set_value name="$kAAIT_test_target_isRepairQueued" exact="this.assignedcontrolled" />
					<include_interrupt_actions ref="kAAIT_GetIsRepairQueued" />
					<do_if value="not $kAAIT_result_isRepairQueued">
						<debug_text text="@this.assignedcontrolled.idcode + ' CancelWaitOrdersWhenOrderChanges signal_object $kAAIT_order_waitforsignal.$releasesignal: ' + @$kAAIT_order_waitforsignal.$releasesignal" chance="if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0" />
						<signal_objects object="$destination" param="$kAAIT_order_waitforsignal.$releasesignal.{1}" param2="@$kAAIT_order_waitforsignal.$releasesignal.{2}" />
					</do_if>
				</do_if>
			</actions>
		</library>
	</add>
	<add pos="before" sel="//create_order[@id=&quot;'WaitForSignal'&quot;][@object=&quot;$destination&quot;]">
		<debug_text text="@this.assignedcontrolled.idcode + ' WaitForSignal $destination: ' + @$destination.knownname + ' ' + @$destination.idcode" chance="if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0" />
	</add>
	<add sel="//create_order[@id=&quot;'WaitForSignal'&quot;][@object=&quot;$destination&quot;]" type="@name">$kAAIT_order_waitforsignal</add>
	<add pos="before" sel="//return[1]">
		<include_interrupt_actions ref="CancelWaitOrdersWhenOrderChanges" />
	</add>
	<add pos="before" sel="//return[2]">
		<include_interrupt_actions ref="CancelWaitOrdersWhenOrderChanges" />
	</add>
	<add sel="//on_abort">
		<debug_text text="@this.assignedcontrolled.idcode + ' on_abort $kAAIT_order_waitforsignal: ' + @$kAAIT_order_waitforsignal" chance="if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0" />
		<debug_text text="@this.assignedcontrolled.idcode + ' on_abort $destination.idcode: ' + $destination.idcode + ' $kAAIT_order_waitforsignal: ' + $kAAIT_order_waitforsignal + ' cancel wait for signal'" chance="if @$destination.idcode == @global.$kAAIT.$debugChance_idCode then @global.$kAAIT_Data.$debugChance else 0" />
		<do_if value="@$kAAIT_order_waitforsignal.exists">
			<!-- <include_interrupt_actions ref="kAAIT_GetIsRepairQueued" /> -->
			<!-- because include_interrupt_actions do not run on_abort, copy kAAIT_GetIsRepairQueued here -->
			<set_value name="$kAAIT_result_isRepairQueued" exact="false" />
			<do_for_each name="$order_inList" in="this.assignedcontrolled.orders">
				<do_if value="$order_inList.script == 'order.repair'">
					<set_value name="$kAAIT_result_isRepairQueued" exact="true" />
					<break />
				</do_if>
			</do_for_each>
			<debug_text text="@this.assignedcontrolled.idcode + ' on_abort $kAAIT_result_isRepairQueued: ' + $kAAIT_result_isRepairQueued" chance="if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0" />
			<do_if value="not $kAAIT_result_isRepairQueued">
				<debug_text text="@this.assignedcontrolled.idcode + ' on_abort signal_object $kAAIT_order_waitforsignal.$releasesignal: ' + @$kAAIT_order_waitforsignal.$releasesignal" chance="if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0" />
				<signal_objects object="$destination" param="$kAAIT_order_waitforsignal.$releasesignal.{1}" param2="@$kAAIT_order_waitforsignal.$releasesignal.{2}" />
			</do_if>
		</do_if>
	</add>
</diff>
