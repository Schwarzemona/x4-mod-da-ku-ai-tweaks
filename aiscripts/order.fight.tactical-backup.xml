		<do_elseif value="$selectedtarget.isclass.ship and this.assignedcontrolled.distanceto.{$selectedtarget} gt this.assignedcontrolled.maxradarrange">
			<create_position name="$kAAIT_orderLocation_pos"
				x="(this.assignedcontrolled.maxradarrange - 5km) * sin ($kAAIT_rot.yaw) * cos ($kAAIT_rot.pitch)"
				y="(this.assignedcontrolled.maxradarrange - 5km) * sin ($kAAIT_rot.pitch)"
				z="(this.assignedcontrolled.maxradarrange - 5km) * cos ($kAAIT_rot.yaw) * cos ($kAAIT_rot.pitch)"
			/>
			<create_position name="$pos" space="$selectedtarget.sector" object="$selectedtarget" value="$kAAIT_orderLocation_pos" />
			<create_order object="this.assignedcontrolled" id="'MoveGeneric'" immediate="true">
				<param name="destination" value="$selectedtarget.sector" />
				<param name="position" value="$pos" />
				<param name="stopondetect" value="$selectedtarget"/>
			</create_order>
			<wait exact="1ms" />
		</do_elseif>
	</add>
	<add pos="after" sel="//label[@name=&quot;targetselection&quot;]">
		<debug_text text="@this.assignedcontrolled.idcode + ' LABEL targetselection'" chance="if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0" />
