				x="(this.assignedcontrolled.maxradarrange + 5km) * sin ($kAAIT_rot.yaw) * cos ($kAAIT_rot.pitch)"
				y="(this.assignedcontrolled.maxradarrange + 5km) * sin ($kAAIT_rot.pitch)"
				z="(this.assignedcontrolled.maxradarrange + 5km) * cos ($kAAIT_rot.yaw) * cos ($kAAIT_rot.pitch)"
			/>
			<create_position name="$pos" space="$selectedtarget.sector" object="$selectedtarget" value="$kAAIT_orderLocation_pos" />
			<create_order object="this.assignedcontrolled" id="'MoveGeneric'" immediate="true">
				<param name="destination" value="$selectedtarget.sector" />
				<param name="position" value="$pos" />
