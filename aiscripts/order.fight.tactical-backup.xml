<!-- add before. purpose: ensure target is in the same sector.
	<label name="targetselection" />
-->
	<add pos="before" sel="//label[@name=&quot;targetselection&quot;]">
		<debug_text text="@this.assignedcontrolled.idcode + ' this.sector: ' + @this.sector.knownname + ' this.assignedcontrolled.sector: ' + @this.assignedcontrolled.sector.knownname + ' $primarytarget.sector: ' + @$primarytarget.sector.knownname" chance="if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0" />
