								</check_all>
							</check_any>
							<set_value name="$kAAIT_isJustFight" exact="true" />
						</check_all>
						<check_all>
							<check_any>
								<event_object_attacked object="$primarytarget" />
								<event_object_attacked_object object="$primarytarget" />
							</check_any>
							<check_value value="this.assignedcontrolled.bboxdistanceto.{$primarytarget} le this.assignedcontrolled.maxradarrange * 0.5" />
							<check_any>
								<check_all>
									<check_value value="event.param == this.assignedcontrolled" />
									<check_value value="@this.assignedcontrolled.allsubordinates.indexof.{event.param}" />
								</check_all>
								<check_all>
									<check_value value="this.sector == $primarytarget.sector" />
									<check_value value="event.param.owner == this.assignedcontrolled.owner" />
									<check_value value="event.param.isclass.ship" />
									<check_value value="not event.param.isunit" />
									<check_value value="not event.param.islasertower" />
									<check_value value="event.param != player.occupiedship" />
								</check_all>
							</check_any>
							<check_any>
								<check_all>
									<check_value value="not $kAAIT_time_justAttack?" />
									<set_value name="$kAAIT_time_resetRallyPoints" exact="player.age" />
								</check_all>
								<check_all>
									<check_value value="$kAAIT_time_justAttack?" />
									<check_value value="player.age gt $kAAIT_time_justAttack + 1s" />
									<set_value name="$kAAIT_time_resetRallyPoints" exact="player.age" />
								</check_all>
							</check_any>
							<set_value name="$kAAIT_isJustFight" exact="true" />
						</check_all>
					</check_any>
				</check_any>
			</conditions>
			<actions>
				<debug_text text="@this.assignedcontrolled.idcode + ' EVENT event.name: ' + event.name + ' event.param: ' + event.param + ' ' + @event.param.knownname + ' ' + @event.param.idcode" chance="if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0" />
				<debug_text text="@this.assignedcontrolled.idcode + ' $kAAIT_isJustFight: ' + @$kAAIT_isJustFight" chance="if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0" />
				<debug_text text="@this.assignedcontrolled.idcode + ' $kAAIT_isResetRallyPoints: ' + @$kAAIT_isResetRallyPoints" chance="if @this.assignedcontrolled.idcode == @global.$kAAIT.$debugChance_idCode or @global.$kAAIT.$debugChance_idCode == 'all' then @global.$kAAIT_Data.$debugChance else 0" />
