<?xml version='1.0' encoding='utf-8'?>
<diff>
  <replace sel="/mdscript/cues/cue[@name='Start']/actions/find_cluster_in_range[@name='$LocalClusters']/@maxdistance">if @md.$DATFindShipSpacesTweak then 3 else 2</replace>
  <replace sel="/mdscript/cues/cue[@name='Start']/cues/cue[@name='EvaluateState']/actions/do_else/do_if[@value='$Mood == moodlevel.verylow']/set_value[@name='$MinimumStrengthFactor'][@exact='1.2f']/@exact">1.0f</replace>
  <replace sel="/mdscript/cues/cue[@name='Start']/cues/cue[@name='EvaluateState']/actions/do_else/do_elseif[@value='$Mood == moodlevel.low']/set_value[@name='$MinimumStrengthFactor'][@exact='1.1f']/@exact">1.0f</replace>
  <replace sel="/mdscript/cues/cue[@name='Start']/cues/cue[@name='EvaluateState']/actions/do_else/do_elseif[@value='$Mood == moodlevel.low']/set_value[@name='$MaximumStrengthFactor'][@exact='1.3f']/@exact">1.4f</replace>
  <replace sel="/mdscript/cues/cue[@name='Start']/cues/cue[@name='EvaluateState']/actions/do_else/do_elseif[@value='$Mood == moodlevel.normal']/set_value[@name='$MaximumStrengthFactor'][@exact='1.4f']/@exact">1.6f</replace>
  <replace sel="/mdscript/cues/cue[@name='Start']/cues/cue[@name='EvaluateState']/actions/do_else/do_all[@exact='$Invasions.count']/do_if[@value='this.$WantedDefendAreaSubgoals gt 0']/do_all[@exact='$Invasions.{$i}.$AlliedEntryPoints.count']/do_if[@value='$Invasions.{$i}.$AlliedEntryPoints.{$k}.exit.sector == $Target']/do_if[@value='not this.$AlreadyDefendingEntryPoint']/find_ship_by_true_owner[@name='this.$OwnShips']/match_distance[@max='20km']/@max">30km</replace>
  <replace sel="/mdscript/cues/cue[@name='Start']/cues/cue[@name='EvaluateState']/actions/do_else/do_all[@exact='$Invasions.count']/do_if[@value='this.$WantedDefendAreaSubgoals gt 0']/do_all[@exact='$Invasions.{$i}.$AlliedEntryPoints.count']/do_if[@value='$Invasions.{$i}.$AlliedEntryPoints.{$k}.exit.sector == $Target']/do_if[@value='not this.$AlreadyDefendingEntryPoint']/find_ship_by_true_owner[@name='this.$EnemyShips']/match_distance[@max='20km']/@max">30km</replace>
  <add sel="/mdscript/cues/cue[@name='Start']/cues/cue[@name='EvaluateState']/actions/do_else/do_for_each[@name='$DesiredSubgoalsKey']/do_if[@value='$DesiredSubgoalAmount gt 0']/do_all[@exact='$DesiredSubgoalAmount']/do_elseif/do_if[@value='$ConstructionPlan']/do_if[@value='$StationMacro']/set_value[@name='$NewSubgoalTable.$DebugChance'][@exact='$DebugChance']" pos="before">
    <do_if value="if md.$DATDefenseStationPositionTweak? then md.$DATDefenseStationPositionTweak else true">
      <set_value name="$DAHoldSpaceGates" exact="[]"/>
      <find_gate name="$DAHoldSpaceGates" space="$NewSubgoalTable.$Target" active="true" multiple="true"/>
      <do_for_each name="$DAHoldSpaceActiveGate" in="$DAHoldSpaceGates">
        <do_if value="$DAHoldSpaceActiveGate.exit.sector.hasrelation.enemy.{$Faction}">
          <set_value name="$DAHoldSpaceDefensiveGate" exact="$DAHoldSpaceActiveGate"/>
          <break/>
        </do_if>
      </do_for_each>
      <do_if value="@$DAHoldSpaceDefensiveGate">
        <get_safe_pos result="$NewSubgoalTable.$Position" sector="$NewSubgoalTable.$Target" radius="5km" object="$DAHoldSpaceDefensiveGate" max="40km" allowyaxis="false"/>
        <remove_value name="$DAHoldSpaceDefensiveGate"/>
      </do_if>
      <do_elseif value="$DAHoldSpaceGates.count gt 1">
        <get_safe_pos result="$NewSubgoalTable.$Position" sector="$NewSubgoalTable.$Target" radius="5km" object="$DAHoldSpaceGates.random" max="40km" allowyaxis="false"/>
        <remove_value name="$DAHoldSpaceGates"/>
      </do_elseif>
      <do_elseif value="$DAHoldSpaceGates.count == 1 and $DAHoldSpaceGates.{1} != null">
        <get_safe_pos result="$NewSubgoalTable.$Position" sector="$NewSubgoalTable.$Target" radius="5km" object="$DAHoldSpaceGates.{1}" max="40km" allowyaxis="false"/>
        <remove_value name="$DAHoldSpaceGates"/>
      </do_elseif>
      <do_else>
        <remove_value name="$DAHoldSpaceGates"/>
      </do_else>
    </do_if>
  </add>
</diff>
